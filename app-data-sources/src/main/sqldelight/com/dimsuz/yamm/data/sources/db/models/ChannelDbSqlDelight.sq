CREATE TABLE channel(
    id TEXT NOT NULL PRIMARY KEY,
    user_id TEXT NOT NULL,
    team_id TEXT NOT NULL,
    type TEXT NOT NULL,
    display_name TEXT,
    name TEXT,
    header TEXT,
    purpose TEXT,
    team_mate_id TEXT
);

insert_channel:
INSERT OR REPLACE INTO channel
VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?);

user_channels:
SELECT *
FROM channel
WHERE user_id = ? AND team_id = ?;
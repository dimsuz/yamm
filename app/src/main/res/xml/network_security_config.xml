<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
     <debug-overrides>
          <trust-anchors>
               <!-- Trust preinstalled CAs -->
               <certificates src="system" />
               <!-- Additionally trust user added CAs (for debugging ssl traffic) -->
               <certificates src="user" />
          </trust-anchors>
     </debug-overrides>
</network-security-config>
